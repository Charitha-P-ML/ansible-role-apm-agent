#<hostname> ansible_host=<host_ip> ansible_user=<instance_username> ansible_ssh_private_key_file=<path to the keyfile> Name=<instance_name> appName=<application_name> projectName=<projectname>
#centos2 ansible_host=192.168.1.10 ansible_user=root ansible_ssh_private_key_file=/etc/ansible/remote-104.key Name=sfagent-ansible appName=ansible projectName=Demo-project

